{
  "canisters": {
    "icrc1_ledger_mock": {
      "main": "test/mocks/icrc1_ledger_mock.mo",
      "type": "motoko",
      "optimize": "cycles",
      "declarations": {
        "bindings": ["js", "ts"],
        "output": "./declarations/icrc1_ledger_mock"
      },
      "init_arg": "(opt \"MCK_1\", opt 6 : opt nat8)"
    },
    "icrc1_ledger_mock_2": {
      "dependencies": ["icrc1_ledger_mock"],
      "wasm": "./.dfx/local/canisters/icrc1_ledger_mock/icrc1_ledger_mock.wasm",
      "candid": "./.dfx/local/canisters/icrc1_ledger_mock/icrc1_ledger_mock.did",
      "type": "custom",
      "init_arg": "(opt \"MCK_2\", opt 2 : opt nat8)"
    },
    "frontend": {
      "type": "assets",
      "dependencies": [
        "icrc1_auction_development"
      ],
      "source": [
        "dist/"
      ],
      "build": "npm run build:frontend",
      "declarations": {}
    },
    "icrc1_auction_development": {
      "main": "src/icrc1_auction_api.mo",
      "did": "src/icrc1_auction.did",
      "type": "motoko",
      "init_arg": "(opt principal \"arufl-lyaaa-aaaao-a3n4a-cai\", opt principal \"2vxsx-fae\", opt principal \"6jrls-gqaaa-aaaao-a4pgq-cai\")",
      "declarations": {
        "bindings": [
          "js",
          "ts"
        ],
        "output": "./declarations/icrc1_auction"
      },
      "optimize": "cycles"
    },
    "crypto": {
      "candid": "crypto_canister/crypto.did",
      "type": "custom",
      "init_arg": "\"key_1\"",
      "shrink": true,
      "wasm": "target/wasm32-unknown-unknown/release/crypto.wasm",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release -p crypto",
        "cargo install candid-extractor --version 0.1.6",
        "candid-extractor target/wasm32-unknown-unknown/release/crypto.wasm > crypto_canister/crypto.did"
      ],
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "declarations": {
        "bindings": [
          "js",
          "ts"
        ],
        "output": "./declarations/crypto"
      }
    },
    "crypto_mock": {
      "candid": "test/mocks/crypto_canister_mock/crypto.did",
      "type": "custom",
      "init_arg": "\"test_key_1\"",
      "shrink": true,
      "wasm": "target/wasm32-unknown-unknown/release/crypto_mock.wasm",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release -p crypto_mock",
        "cargo install candid-extractor --version 0.1.6",
        "candid-extractor target/wasm32-unknown-unknown/release/crypto_mock.wasm > test/mocks/crypto_canister_mock/crypto.did"
      ],
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "dfx": "0.29.0",
  "version": 1
}
